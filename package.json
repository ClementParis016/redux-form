{
  "name": "redux-form-website",
  "version": "0.0.1",
  "description": "The generator for the redux-form website",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/redux-form"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "publish": "node lib/publish.js"
  },
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikras/redux-form/issues"
  },
  "homepage": "https://redux-form.com",
  "dependencies": {
    "http-client": "^4.3.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.forin": "^4.0.1",
    "mkdirp": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "redux-form-website-template": "0.0.56"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cross-env": "^5.0.1",
    "eslint": "^4.2.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "rimraf": "^2.4.3",
    "webpack": "^3.1.0"
  },
  "npmName": "redux-form-website-generator",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
